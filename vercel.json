{
  "rewrites": [
    {
      "source": "/pokemon",
      "destination": "/api/pokemon.js"
    },
    {
      "source": "/pokemon/:path*",
      "destination": "/api/pokemon.js"
    },
    {
      "source": "/detail",
      "destination": "/api/detail.js"
    },
    {
      "source": "/detail/:path*",
      "destination": "/api/detail.js"
    }
  ]
}